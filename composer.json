{
    "name": "rtsachev/code-analyzer",
    "description": "Package to wrap some static code analysis tools.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rostislav Tsachev",
            "email": "ross102t@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "phpunit/phpunit": "~6.0",
        "pdepend/pdepend": "2.5.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "3.*",
        "sebastian/phpcpd": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "RTsachev\\CodeAnalyzer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RTsachev\\CodeAnalyzer\\CodeAnalyzerServiceProvider"
            ]
        }
    }
}
